omponent: Layout,\n        icon: \"cloud-server-outlined\",\n        children: [\n            {\n                path: \"/cluster/node\",\n                name: \"Node\",\n                meta: {title: \"Node\", requireAuth: true},\n                component: () => import('@/views/cluster/Node.vue'),\n            },\n            {\n                path: \"/cluster/namespace\",\n                name: \"Namespace\",\n                meta: {title: \"Namespace\", requireAuth: true},\n                component: () => import('@/views/cluster/Namespace.vue'),\n            },\n            {\n                path: \"/cluster/pv\",\n                name: \"PV\",\n                meta: {title: \"PV\", requireAuth: true},\n                component: () => import('@/views/cluster/PV.vue'),\n            }\n        ]\n    }, \n     {\n        path: \"/workload\",\n        name: \"工作负载\",\n        component: Layout,\n        icon: \"block-outlined\",\n        children: [\n            {\n                path: \"/workload/pod\",\n                name: \"Pod\",\n                meta: {title: \"Pod\", requireAuth: true},\n                component: () => import('@/views/workload/Pod.vue'),\n            },\n            {\n                path: \"/workload/deployment\",\n                name: \"Deployment\",\n                meta: {title: \"Deployment\", requireAuth: true},\n                component: () => import('@/views/workload/Deployment.vue'),\n            },\n            {\n                path: \"/workload/daemonset\",\n                name: \"DaemonSet\",\n                meta: {title: \"DaemonSet\", requireAuth: true},\n                component: () => import('@/views/workload/DaemonSet.vue'),\n            },\n            {\n                path: \"/workload/statefulset\",\n                name: \"StatefulSet\",\n                meta: {title: \"StatefulSet\", requireAuth: true},\n                component: () => import('@/views/workload/StatefulSet.vue'),\n            },\n        ]\n    },\n]\n\n//创建路由实例\nconst router = createRouter({\n    history: createWebHistory(),\n    routes\n})\n\n//定义进度条\nNProgress.inc(100)\n// 进度条配置\n// easing 动画字符串\n// speed 动画速度\n// showSpinner 金都环显示隐藏\nNProgress.configure({easing: 'ease',speed: 600,showSpinner: false})\n\n//结合路由守卫，去开启和关闭进度条\nrouter.beforeEach((to,form,next)=> {\n    //1、启动进度条\n    NProgress.start()\n    //2、设置头部title\n    if (to.meta.title){\n        //可以拿到上面连接的 title meta: {title: \"StatefulSet\", requireAuth: true},\n        document.title = to.meta.title //固定写法\n    }else{\n        document.title = \"kubeA\"\n    }\n    //放行\n    next()\n})\n//在你加载完这个页面后关闭进度条\nrouter.afterEach(()=>{\n    // 关闭进度条\n    NProgress.done()\n})\nexport default router"],"mappings":"AAAA;AACA;;AAEA,SAAQA,YAAY,EAACC,gBAAgB,QAAO,YAAY;AACxD;AACA,OAAOC,SAAS,MAAM,WAAW;AACjC,OAAO,yBAAyB;;AAEhC;AACA,OAAOC,MAAM,MAAM,iBAAiB;;AAEpC;;AAEA,MAAMC,MAAM,GAAG,CACX;EACQC,IAAI,EAAE,GAAG;EACTC,QAAQ,EAAE,OAAO,CAAE;AAE3B,CAAC,EACD;EACAD,IAAI,EAAE,OAAO;EACb;EACAE,SAAS,EAAEJ,MAAM;EACjBK,QAAQ,EAAE,CACN;IACIH,IAAI,EAAE,OAAO;IACbI,IAAI,EAAE,IAAI;IACVC,IAAI,EAAE,eAAe;IACrBC,IAAI,EAAE;MAACC,KAAK,EAAE;IAAI,CAAC;IACnB;IACAL,SAAS,EAAEA,CAAA,KAAM,MAAM,CAAC,uBAAuB;EACnD,CAAC;AAEL,CAAC,EACD;EACIF,IAAI,EAAE,UAAU;EAChBI,IAAI,EAAE,IAAI;EACVF,SAAS,EAAEJ,MAAM;EACjBO,IAAI,EAAE,uBAAuB;EAC7BF,QAAQ,EAAE,CACN;IACIH,IAAI,EAAE,eAAe;IACrBI,IAAI,EAAE,MAAM;IACZE,IAAI,EAAE;MAACC,KAAK,EAAE,MAAM;MAAEC,WAAW,EAAE;IAAI,CAAC;IACxCN,SAAS,EAAEA,CAAA,KAAM,MAAM,CAAC,0BAA0B;EACtD,CAAC,EACD;IACIF,IAAI,EAAE,oBAAoB;IAC1BI,IAAI,EAAE,WAAW;IACjBE,IAAI,EAAE;MAACC,KAAK,EAAE,WAAW;MAAEC,WAAW,EAAE;IAAI,CAAC;IAC7CN,SAAS,EAAEA,CAAA,KAAM,MAAM,CAAC,+BAA+B;EAC3D,CAAC,EACD;IACIF,IAAI,EAAE,aAAa;IACnBI,IAAI,EAAE,IAAI;IACVE,IAAI,EAAE;MAACC,KAAK,EAAE,IAAI;MAAEC,WAAW,EAAE;IAAI,CAAC;IACtCN,SAAS,EAAEA,CAAA,KAAM,MAAM,CAAC,wBAAwB;EACpD,CAAC;AAET,CAAC,EACA;EACGF,IAAI,EAAE,WAAW;EACjBI,IAAI,EAAE,MAAM;EACZF,SAAS,EAAEJ,MAAM;EACjBO,IAAI,EAAE,gBAAgB;EACtBF,QAAQ,EAAE,CACN;IACIH,IAAI,EAAE,eAAe;IACrBI,IAAI,EAAE